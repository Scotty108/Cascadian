# Cascadian App - Environment Variables Example
# Copy this file to .env.local and fill in your values

# ============================================================================
# REQUIRED - Supabase Configuration
# ============================================================================
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# ============================================================================
# REQUIRED - OpenAI API (for AI Copilot)
# ============================================================================
OPENAI_API_KEY=your_openai_api_key

# ============================================================================
# OPTIONAL - Real Polymarket Data Integration
# ============================================================================
# Enable real Polymarket data in workflow execution (default: false = stub data)
NEXT_PUBLIC_USE_REAL_POLYMARKET=true

# API URL for workflow execution (default: empty = relative URLs)
NEXT_PUBLIC_API_URL=http://localhost:3009

# ============================================================================
# OPTIONAL - Google AI (for LLM nodes)
# ============================================================================
GOOGLE_GENERATIVE_AI_API_KEY=your_google_ai_key
